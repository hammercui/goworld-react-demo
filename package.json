    "antd-pro-merge-less": "^0.1.0",
    "antd-theme-webpack-plugin": "^1.1.8",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
